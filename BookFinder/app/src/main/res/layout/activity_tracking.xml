<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_tracking"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="group151.bookfinder.TrackingActivity">

    <TextView
        android:text="Shipping Carrier"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ShippingCarrier"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:fontFamily="sans-serif"
        android:typeface="normal"
        android:textSize="14sp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="USPS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/CarrierValue"
        android:layout_below="@+id/ShippingCarrier"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="28dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:fontFamily="sans-serif" />

    <TextView
        android:text="Tracking Number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/TrackingNumber"
        android:layout_marginTop="63dp"
        android:layout_below="@+id/CarrierValue"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:fontFamily="sans-serif" />

    <TextView
        android:text="1214500846165448975213613"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tracking_number"
        android:layout_marginTop="19dp"
        android:layout_below="@+id/TrackingNumber"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:fontFamily="sans-serif" />
</RelativeLayout>
